}
StopwatchWindow QListWidget::item:selected {
    background-color: #D52941;
    color: #FFF8E8;
}

StopwatchWindow QListWidget::item[minLap="true"] {
    background-color: rgba(65, 82, 31, 0.25);
}

StopwatchWindow QListWidget::item[maxLap="true"] {
    background-color: rgba(153, 13, 53, 0.25);
}

QToolTip {
    background-color: #FFF8E8;
    border: 1px solid #FCD581;
    color: #41521F;
    font-family: "Open Sans";
    font-size: 12px;
    border-radius: 6px;
    padding: 5px 8px;
}


