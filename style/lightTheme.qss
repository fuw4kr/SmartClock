                                stop:0 #FFF8E8, stop:1 #FCD581);
    border: 1px solid #FCD581;
    border-radius: 10px;
    padding: 8px 18px;
    font-family: "Poppins SemiBold";
    font-size: 14px;
    color: #41521F;
    transition: all 0.25s ease;
}

StopwatchWindow QPushButton#btnStartStop:hover,
StopwatchWindow QPushButton#btnLap:hover {
    background-color: #D52941;
    color: #FFF8E8;
}

StopwatchWindow QPushButton#btnStartStop:pressed,
StopwatchWindow QPushButton#btnLap:pressed {
    background-color: #990D35;
    color: #FFF8E8;
}

StopwatchWindow QPushButton {
    border: none;
    border-radius: 6px;
    background: transparent;
}
StopwatchWindow QPushButton:hover {
    background-color: rgba(252, 213, 129, 0.25);
}

StopwatchWindow QListWidget#listLaps {
    background-color: #FFF8E8;
    border: 1px solid #FCD581;
    border-radius: 8px;
    font-family: "Open Sans";
    font-size: 14px;
    color: #41521F;
    padding: 4px;
}
StopwatchWindow QListWidget::item {
