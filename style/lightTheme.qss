    color: #FFF8E8;
}

ClockWindow QPushButton#btnAddClock,
ClockWindow QPushButton#btnRemoveClock {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1,
                                stop:0 #FFF8E8, stop:1 #FCD581);
    border: 1px solid #FCD581;
    border-radius: 8px;
    padding: 6px 16px;
