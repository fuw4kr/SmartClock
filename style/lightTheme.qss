}

HistoryTimerWindow QTableWidget::item:hover {
    background-color: rgba(252, 213, 129, 0.35);
}

HistoryTimerWindow QTableWidget::item:selected {
    background-color: #F6C6C3;
    color: #2B2B2B;
}

HistoryTimerWindow QTableWidget QPushButton {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1,
                                stop:0 #FFFBE8, stop:1 #FCD581);
    border: 1px solid #E6C76A;
    border-radius: 6px;
    padding: 3px 10px;
    color: #41521F;
    font-family: "Poppins SemiBold";
    font-size: 12px;
}

HistoryTimerWindow QTableWidget QPushButton:hover {
    background: #D52941;
    color: #FFF8E8;
}

HistoryTimerWindow QTableWidget QPushButton:pressed {
    background: #990D35;
    color: #FFF8E8;
}


/* ---------- BUTTONS ---------- */
QPushButton {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1,
                                stop:0 #FFF8E8, stop:1 #FCD581);
    border: 1px solid #FCD581;
    border-radius: 10px;
    padding: 6px 14px;
    color: #41521F;
    font-family: "Poppins SemiBold";
    font-size: 13px;
    transition: all 0.25s ease;
}
QPushButton:hover {
    background-color: #D52941;
    color: #FFF8E8;
}
QPushButton:pressed {
    background-color: #990D35;
    color: #FFF8E8;
}

/* ---------- COMBOBOXES ---------- */
QComboBox {
    background-color: #fffdf8;
    border: 1px solid #FCD581;
    border-radius: 6px;
    padding: 4px 6px;
    font-family: "Open Sans";
    font-size: 13px;
    color: #41521F;
}
QComboBox::drop-down {
    border: none;
    width: 14px;
    background: transparent;
}

/* ---------- CHECKBOXES ---------- */
QCheckBox {
    font-family: "Open Sans";
    font-size: 13px;
    color: #41521F;
}
QCheckBox::indicator {
    width: 16px;
    height: 16px;
    border-radius: 4px;
    border: 1px solid #FCD581;
    background: #FFF8E8;
}
QCheckBox::indicator:checked {
    background-color: #D52941;
    image: url(:/resources/icons/checkmark.png);
}

/* ---------- GROUPBOXES ---------- */
QGroupBox {
    border: 1px solid #FCD581;
    border-radius: 8px;
    margin-top: 8px;
    font-family: "Poppins SemiBold";
    font-size: 12px;
    color: #41521F;
}
QGroupBox::title {
    subcontrol-origin: margin;
    left: 8px;
    padding: 0 3px;
    background-color: #FFF8E8;
}

/* ---------- SPINBOX, LINEEDIT, TIMEEDIT ---------- */
QSpinBox, QLineEdit, QDateTimeEdit {
    background-color: #fffdf8;
    border: 1px solid #FCD581;
    border-radius: 6px;
    padding: 3px 6px;
    font-family: "Open Sans";
    font-size: 13px;
    color: #41521F;
}

/* ---------- DIALOG BUTTON BOX ---------- */
QDialogButtonBox QPushButton {
    min-width: 80px;
}

/* ---------- SCROLLBARS ---------- */
QScrollBar:vertical {
    background: #FFF8E8;
    width: 12px;
    margin: 0px;
}
QScrollBar::handle:vertical {
    background: #FCD581;
    border-radius: 5px;
}
QScrollBar::handle:vertical:hover {
    background: #D52941;
}
QScrollBar::add-line:vertical,
QScrollBar::sub-line:vertical {
    border: none;
    background: none;
    height: 0px;
}

/* ---------- TOOLTIP ---------- */
QToolTip {
    background-color: #FFF8E8;
    border: 1px solid #FCD581;
    color: #41521F;
    font-family: "Open Sans";
    font-size: 12px;
    border-radius: 6px;
    padding: 5px 8px;
}
