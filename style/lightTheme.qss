    height: 18px;
    border-radius: 4px;
    border: 1px solid #FCD581;
    background: #FFF8E8;
}

ClockWindow QCheckBox::indicator:checked {
    background: #D52941;
    image: url(:/resources/icons/checkmark.png);
}

ClockWindow QListWidget#listClocks {
    background-color: #FFF8E8;
    border: 1px solid #FCD581;
    border-radius: 8px;
    padding: 4px;
