    background: transparent;
}


QCheckBox {
    font-family: "Open Sans";
    font-size: 13px;
    color: #41521F;
}
QCheckBox::indicator {
    width: 16px;
    height: 16px;
    border-radius: 4px;
    border: 1px solid #FCD581;
    background: #FFF8E8;
}
QCheckBox::indicator:checked {
    background-color: #D52941;
    image: url(:/resources/icons/checkmark.png);
}


QGroupBox {
    border: 1px solid #FCD581;
    border-radius: 8px;
    margin-top: 8px;
    font-family: "Poppins SemiBold";
    font-size: 12px;
    color: #41521F;
}
QGroupBox::title {
    subcontrol-origin: margin;
    left: 8px;
    padding: 0 3px;
    background-color: #FFF8E8;
}


QSpinBox, QLineEdit, QDateTimeEdit {
    background-color: #fffdf8;
    border: 1px solid #FCD581;
    border-radius: 6px;
    padding: 3px 6px;
    font-family: "Open Sans";
    font-size: 13px;
    color: #41521F;
}


QDialogButtonBox QPushButton {
    min-width: 80px;
}


QScrollBar:vertical {
    background: #FFF8E8;
    width: 12px;
    margin: 0px;
}
QScrollBar::handle:vertical {
    background: #FCD581;
    border-radius: 5px;
}
QScrollBar::handle:vertical:hover {
    background: #D52941;
}
QScrollBar::add-line:vertical,
QScrollBar::sub-line:vertical {
    border: none;
    background: none;
    height: 0px;
}


QToolTip {
    background-color: #FFF8E8;
    border: 1px solid #FCD581;
    color: #41521F;
    font-family: "Open Sans";
    font-size: 12px;
    border-radius: 6px;
    padding: 5px 8px;
}


QComboBox {
    background-color: #ffffff;
    border: 1px solid #d8dce6;
    border-radius: 8px;
    padding: 6px 10px;
    color: #222;
}

QComboBox:hover {
    border: 1px solid #b9b4ff;
}

QComboBox:focus {
    border: 1px solid #6c63ff;
}

QComboBox::drop-down {
    subcontrol-origin: padding;
    subcontrol-position: top right;
    width: 24px;
    border-left: 1px solid #d8dce6;
    background-color: #f1f3f9;
    border-top-right-radius: 8px;
    border-bottom-right-radius: 8px;
}

QComboBox::down-arrow {
    image: url(:/resources/icons/sortdownL.png);
    width: 20px;
    height: 20px;
}

QComboBox QAbstractItemView {
    background-color: #ffffff;
    border: 1px solid #d8dce6;
    selection-background-color: #e8ebf7;
    selection-color: #2e2bff;
    border-radius: 6px;
}

QComboBox QAbstractItemView::item {
    height: 20px;
    padding: 4px 10px;
}


 QSpinBox {
    background-color: #ffffff;
    border: 1px solid #cccccc;
    border-radius: 6px;
    padding: 4px 8px;
    color: #222;
}
 QSpinBox::up-button {
    image: url(:/resources/icons/sortupL.png);
    width: 20px;
    height: 10px;
    border: 1px solid #cccccc;
    border-radius: 5px;
}

 QSpinBox::down-button {
    image: url(:/resources/icons/sortdownL.png);
    width: 20px;
    height: 10px;
    border: 1px solid #cccccc;
    border-radius: 5px;
}

 QDateTimeEdit::down-button{
    image: url(:/resources/icons/sortdownL.png);
    width: 20px;
    height: 10px;
    border: 1px solid #cccccc;
    border-radius: 5px;
}

 QDateTimeEdit::up-button {
     image: url(:/resources/icons/sortupL.png);
     width: 20px;
     height: 10px;
     border: 1px solid #cccccc;
     border-radius: 5px;
}

 QRadioButton {
    color: #1e1e1e;
    font-size: 14px;
    spacing: 6px;
}

 QRadioButton::indicator {
    width: 16px;
    height: 16px;
    border-radius: 8px;
    border: 2px solid #bbb;
    background-color: white;
}

 QRadioButton::indicator:hover {
    border-color: #2563eb;
}

 QRadioButton::indicator:checked {
    border-color: #2563eb;
    background-color: #2563eb;
}
